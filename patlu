#include<stdio.h>
void main()
{
    int i=1,n;
    scanf("%d",&n);
    while(n>0)
    {
        n=n-i;
        if(n<=0)
        {
            printf("Patlu");
            break;
        }
        n=n-(2*i);
        if(n<=0)
        {
            printf("Motu");
            break;
        }
        i++;
    }
}
